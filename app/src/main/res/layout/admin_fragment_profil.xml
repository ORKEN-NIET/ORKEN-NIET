<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:orientation="vertical">
       <EditText
           android:id="@+id/edt_aty"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="Aty_jon"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/edt_familia"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="familia"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/edt_akesinin_aty"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="akesinin"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/editTextEmail"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="email"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/editTextPassword"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="email"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/edt_zhynysu"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="zhynysy"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/edt_status"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="status"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/edt_ulty"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="ulty"
           android:textColor="@color/black"/>
       <EditText
           android:id="@+id/edt_tugan_kun"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="15dp"
           android:text="Tugan_kun"
           android:textColor="@color/black"/>
   </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>